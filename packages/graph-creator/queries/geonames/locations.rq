# A 'GROUP BY ?iri' does not produce the desired results on production
SELECT DISTINCT ?iri
WHERE {
  {
    SELECT ?iri
    WHERE {
      GRAPH ?g {
        ?s ?p ?iri
        FILTER(isIRI(?iri) && REGEX(STR(?iri), "^https://sws.geonames.org/\\d+/$"))
      }
      # Exclude results from the GeoNames graph
      FILTER(?g != <https://data.colonialcollections.nl/geonames>)
    }
  }
}
LIMIT ?_limit
OFFSET ?_offset
