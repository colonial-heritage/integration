# A 'GROUP BY ?iri' does not produce the desired results on production
SELECT DISTINCT ?iri
WHERE {
  {
    SELECT ?iri
    WHERE {
      GRAPH ?g {
        ?s ?p ?iri
        FILTER(isIRI(?iri) && REGEX(STR(?iri), "^http://vocab.getty.edu/aat/\\d+$"))
      }
      # Exclude results from the AAT graph
      FILTER(?g != <https://data.colonialcollections.nl/aat>)
    }
  }
}
LIMIT ?_limit
OFFSET ?_offset
